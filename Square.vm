
push argument 0
pop local 0

// Initialize local 1 to 0 (sum accumulator)
push constant 0
pop local 1

// Start of loop
(LOOP)
push local 0          
push constant 0       
eq                    
if-goto END           


push local 0
push local 1
add
pop local 1        


push local 0
push constant 1
sub
pop local 0       


goto LOOP

(END)
push local 1
